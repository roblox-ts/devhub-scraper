            "children": [
                {
                    "type": "text",
                    "value": "This property modifies the lighting of the surface by adding bumps, dents,\ncracks, and curves without adding more polygons.",
                    "position": {
                        "start": {
                            "line": 1,
                            "column": 1,
                            "offset": 0
                        },
                        "end": {
                            "line": 2,
                            "column": 49,
